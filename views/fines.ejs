<h1>All Students payment</h1>
<% if (fines.length > 0) { %>
  <table>
    <thead>
      <tr>
        <th>Student Name </th>
        <th>Registration Number</th>
        <th>Email</th>
        <th>Course</th>
        <th>Fine Name</th>
        <th>Fine Amount</th>
      </tr>
    </thead>
    <tbody>
      <% fines.forEach(function(fine) { %>
        <tr>
          <td><%= fine.studentname %></td>
          <td><%= fine.regno %></td>
          <td><%= fine.email %></td>
          <td><%= fine.course %></td>
          <td><%= fine.studentname %></td>
          <td><%= fine.amount %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
<% } else { %>
  <p>No fines found.</p>
<% } %>